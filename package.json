{
  "name": "obstetrics",
  "version": "0.0.0",
  "description": "Obstetrics- A Pregnancy Data Collection System",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm start",
    "dev:ssr": "ng run obstetrics:serve-ssr",
    "serve:ssr": "node dist/obstetrics/server/main.js",
    "build:ssr": "ng build --prod && ng run obstetrics:server:production",
    "prerender": "ng run obstetrics:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/platform-server": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@nguniversal/express-engine": "^11.2.1",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "2.9.4",
    "express": "^4.15.2",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "rxjs": "~6.6.0",
    "tailwindcss": "2.0.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/language-service": "11.2.5",
    "@nguniversal/builders": "^11.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
